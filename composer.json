{
    "name": "dunglas/digital-ocean-bundle",
    "description": "DigitalOcean API v2 client for Symfony and API Platform",
    "keywords": ["DigitalOcean", "symfony", "API", "Cloud Hosting", "SSD", "VPS"],
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dunglas\\DigitalOceanBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Dunglas\\DigitalOceanBundle\\Tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "kevin@dunglas.fr"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^4.4 || ^5.4 || ^6.4 || ^7.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.4 || ^7.0",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.4 || ^7.0",
        "toin0u/digitalocean-v2": "^4.3"
    },
    "require-dev": {
        "guzzlehttp/promises": "^1.4 || ^2.0",
        "nyholm/psr7": "^1.4",
        "symfony/http-client": "^4.4 || ^5.4 || ^6.4 || ^7.0",
        "symfony/phpunit-bridge": "^6.4 || ^7.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
